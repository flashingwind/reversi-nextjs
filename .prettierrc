	{
		"printWidth": 100,
		"trailingComma": "es5",
		"tabWidth": 2,
		"semi": true,
		"singleQuote": true,
		"endOfLine": "lf",

		"useTabs": false,
		"quoteProps": "as-needed",
		"jsxSingleQuote": true,
		"bracketSpacing": true,
		"jsxBracketSameLine": false,
		"arrowParens": "always",
		"rangeStart": 0,
        "rangeEnd": 99999999,
		"proseWrap": "always",
		"htmlWhitespaceSensitivity": "css",
		"vueIndentScriptAndStyle": false
    }
